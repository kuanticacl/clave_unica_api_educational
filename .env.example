# Example environment variables for the application

# Redis configuration (for local development)
REDISHOST=localhost
REDISPORT=6379
REDISPASSWORD="" # Leave empty if no password
REDIS_DB=0

# Rate limiting
RATE_LIMIT_TIMES_SCRAPE=1
RATE_LIMIT_SECONDS_SCRAPE=60
RATE_LIMIT_TIMES_HEALTH=6
RATE_LIMIT_SECONDS_HEALTH=60

# Logging
LOG_WITH_TIMESTAMP=true

# Railway specific variables (provided by Railway, no need to set manually)
# RAILWAY_RUN_AS_ROOT
# RAILWAY_RUN_UID
# REDIS_AOF_ENABLED
# REDIS_PASSWORD
# REDIS_PUBLIC_URL
# REDIS_RDB_POLICY
# REDIS_URL
# REDISUSER